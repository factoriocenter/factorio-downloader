# ------------------------------------------------------------------
# PHP Project .gitignore
# ------------------------------------------------------------------

# 1) Log files and caches
*.log
*.cache
*.tmp
*.lock

# 2) Common development directories
node_modules/
vendor/

# 3) IDE and editor settings
.idea/
.vscode/

# 4) System files
.DS_Store
Thumbs.db

